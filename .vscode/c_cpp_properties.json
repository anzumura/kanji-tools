{
  "env": {
    "google": "${workspaceFolder}/build/_deps/googletest-src",
    "tools": "/Library/Developer/CommandLineTools",
    "sdk": "${tools}/SDKs/MacOSX.sdk/usr/include"
  },
  "configurations": [
    {
      "name": "Mac",
      "compilerPath": "/usr/bin/clang++",
      "cppStandard": "c++20",
      "includePath": [
        "${workspaceFolder}/include",
        "${google}/googletest/include",
        "${google}/googletest",
        "${google}/googlemock/include",
        // "/opt/icu/include",
        // "/opt/boost/include",
        "${sdk}/c++/v1",
        "${sdk}",
        "${tools}/usr/lib/clang/13.1.6/include"
      ],
      "intelliSenseMode": "macos-clang-arm64"
    }
  ],
  "version": 4
}