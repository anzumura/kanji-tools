# Doxyfile 1.9.4

# cmake runs 'doxygen DoxyFile' from the top directory so set INPUT to 'libs'
# to get all relevant C++ code. The output is generated under build/docs/html.

DOXYFILE_ENCODING      = UTF-8
GENERATE_LATEX         = NO
INPUT                  = libs
OUTPUT_DIRECTORY       = build/docs
OUTPUT_LANGUAGE        = English
PROJECT_BRIEF          = "Tools for working with Japanese Kana and Kanji"
PROJECT_NAME           = "Kanji Tools"
PROJECT_NUMBER         = 1.0
QUIET                  = YES
RECURSIVE              = YES
TAB_SIZE               = 2
WARN_IF_INCOMPLETE_DOC = NO
WARN_IF_UNDOCUMENTED   = NO
WARNINGS               = NO
