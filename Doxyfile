# CALLER_GRAPH           = YES
# EXTRACT_ANON_NSPACES   = YES
# EXTRACT_PRIVATE        = YES
# EXTRACT_PRIV_VIRTUAL   = YES
# EXTRACT_STATIC         = YES
# INTERNAL_DOCS          = YES # show docs after \internal
